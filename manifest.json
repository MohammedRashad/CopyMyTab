{
    "name": "CopyMyTabs",
    "version": "1.0",
    "description" : "Awesome Shit to automate your life a little bit...",
    "manifest_version": 2,
    "permissions": ["tabs",
		    "<all_urls>",
		    "unlimitedStorage",
		    "http://*/*",
		    "https://*/*",
		    "clipboardRead",
		    "clipboardWrite"],
    "browser_action": {
        "default_title": "TabCopy",
	"default_icon" : "icons/icon.png",
        "default_popup": "html/popup.html",
	"icons": { "16": "icons/icon.png"}
    } 
}
